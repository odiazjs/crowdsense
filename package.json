        "babel-loader": "^8.0.5",
        "chart.js": "^2.8.0",
        "clean-webpack-plugin": "^1.0.0",
        "copy-webpack-plugin": "^4.6.0",
        "cross-env": "^5.2.0",
        "css-loader": "^0.28.11",
        "eslint": "^4.19.1",
        "eslint-config-airbnb-base": "^13.0.0",
        "eslint-loader": "^2.0.0",
        "eslint-plugin-import": "^2.13.0",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.4.1",
